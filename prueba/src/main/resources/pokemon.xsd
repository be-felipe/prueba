<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://example.com/pokemon/webservice" targetNamespace="http://example.com/pokemon/webservice" elementFormDefault="qualified">
    <xs:element name="getPokemonRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="offset" type="xs:int"/>
                <xs:element name="limit" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="getPokemonResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="pokemons" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
